{
    "extends": [
        "config:base",
        ":automergeBranch",
        ":automergePatch",
        ":automergeTypes"
    ],
    "packageRules": [
        {
            "matchPackagePatterns": "^@?parcel(/|$)",
            "groupName": ["parcel packages"]
        }
    ],
    "nvm": {
        "fileMatch": ["^\\.node-version$"]
    },
    "rangeStrategy": "bump"
}
