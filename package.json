{
    "name": "@marnes/webextensions",
    "version": "0.0.0",
    "private": true,
    "license": "MPL-2.0",
    "repository": "https://gitlab.com/Marnes/webextensions",
    "author": "Marnes <leaumar@mailbox.org>",
    "scripts": {
        "build": "yarn wsrun -s build",
        "bundle": "bestzip sources.zip \"extensions/*/*.*\" \"extensions/*/src\" .editorconfig .eslintrc.react.yml .eslintrc.yml .gitignore .node-version .prettierrc.yml .yarnrc.yml browserslist license.md package.json readme.md tsconfig.json web-ext-config.js yarn.lock",
        "clean": "rimraf sources.zip && yarn wsrun -s clean",
        "prettier": "prettier --write --list-different --ignore-unknown --ignore-path .gitignore . !.yarn !yarn.lock"
    },
    "devDependencies": {
        "@typescript-eslint/parser": "^5.4.0",
        "babel-types": "^6.26.0",
        "bestzip": "^2.2.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-react": "^1.1.7",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-node": "^0.3.6",
        "eslint-plugin-import": "^2.25.3",
        "prettier": "^2.4.1",
        "prettier-plugin-package": "^1.3.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.4.4",
        "wsrun": "^5.2.4"
    },
    "resolutions": {
        "@types/react": "^17.0.37"
    },
    "dependenciesMeta": {
        "fsevents": {
            "built": false
        },
        "parcel-bundler": {
            "built": false
        }
    },
    "packageManager": "yarn@2.4.3",
    "workspaces": [
        "extensions/*"
    ]
}
