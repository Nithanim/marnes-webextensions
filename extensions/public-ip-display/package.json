{
    "name": "public-ip-display",
    "version": "0.0.0",
    "private": true,
    "license": "MPL-2.0",
    "repository": "https://gitlab.com/Marnes/public-ip-display.git",
    "author": "Marnes <leaumar@mailbox.org>",
    "scripts": {
        "build": "yarn clean && yarn tsc && yarn eslint && yarn svelte-check && yarn compile && yarn qa && yarn package",
        "clean": "rimraf dist extension",
        "compile": "yarn flags && yarn parcel build \"src/*.*\" \"src/images/*.*\" --dist-dir extension",
        "compile:watch": "yarn flags && yarn parcel watch \"src/*.*\" \"src/images/*.*\" --dist-dir extension --no-autoinstall --no-hmr",
        "eslint": "eslint \"src/**/*.{ts,tsx}\"",
        "flags": "yarn rimraf extension/images/flags && yarn cpy \"src/images/flags/*.svg\" extension/images/flags",
        "package": "web-ext build",
        "qa": "web-ext lint",
        "start": "web-ext run",
        "start:chrome": "web-ext run -t chromium",
        "tsc": "tsc"
    },
    "dependencies": {
        "fast-equals": "^3.0.0",
        "modern-normalize": "^1.1.0",
        "tachyons": "^4.12.0",
        "ts-essentials": "^9.1.2",
        "tslib": "^2.3.1",
        "type-fest": "^2.12.0",
        "webextension-polyfill": "^0.8.0"
    },
    "devDependencies": {
        "@types/webextension-polyfill": "^0.8.2",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "cpy-cli": "^3.1.1",
        "eslint": "^8.9.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-svelte3": "^3.4.1",
        "parcel": "^2.3.2",
        "parcel-transformer-svelte": "^1.2.3",
        "prettier": "^2.5.1",
        "prettier-plugin-package": "^1.3.0",
        "prettier-plugin-svelte": "^2.6.0",
        "rimraf": "^3.0.2",
        "svelte": "^3.46.4",
        "svelte-check": "^2.4.5",
        "svelte-preprocess": "^4.10.4",
        "typescript": "^4.5.5",
        "web-ext": "^6.7.0"
    }
}
